[alias]
	gui = !sh -c '/usr/local/git/libexec/git-core/git-gui'
 	st = status
	ci = commit -m 
	br = branch
	co = checkout
	create = checkout -b
	df = diff
	lg = log -p
	master = checkout master
	develop = checkout develop
	po = push nzack HEAD
	poo = push origin HEAD
	ftch = fetch --verbose
  ff = merge --ff-only
	rh = reset --hard HEAD
	up = "!f() { git branch --set-upstream-to=origin/$1 $1; }; f"
	cl = "!f() { git clone git@github.com:$1; }; f"
	remote-add = "!f() { git remote add $1 git@github.com:$2/$3.git; }; f"


